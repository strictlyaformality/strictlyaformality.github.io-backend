{
  "kind": "collectionType",
  "collectionName": "skills",
  "info": {
    "name": "Skill"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "skillCategory": {
      "model": "skill-category",
      "via": "skills"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    }
  }
}
